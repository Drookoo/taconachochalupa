PH_UWSGI_LOG_FILE {
    compress
    weekly
    rotate 10
    create 0600 PH_SYS_ROOT_USER PH_SYS_ROOT_GROUP
    missingok
    notifempty

    # Use bzip2 for compress.
    #compresscmd bzip2
    #uncompresscmd bunzip2
    #compressoptions -9
    #compressext .bz2

    postrotate
        PH_SYSLOG_POSTROTATE_CMD
    endscript
}

